{
  "extends": ["prettier", "node"],
  "plugins": ["prettier", "node"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "script",
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": ["@babel/plugin-syntax-jsx"]
    }
  },
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
